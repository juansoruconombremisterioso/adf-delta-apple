{
	"name": "df_silver_sales",
	"properties": {
		"folder": {
			"name": "SILVER"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_sales",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_silver_warranty",
						"type": "DatasetReference"
					},
					"name": "source3"
				},
				{
					"dataset": {
						"referenceName": "ds_silver_products",
						"type": "DatasetReference"
					},
					"name": "source4"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_silver_sales",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "cast1"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          sale_id as string,",
				"          sale_date as string,",
				"          store_id as string,",
				"          product_id as string,",
				"          quantity as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source(output(",
				"          claim_id as integer,",
				"          claim_date as date,",
				"          sale_id as string,",
				"          repair_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source3",
				"source(output(",
				"          product_id as integer,",
				"          product_name as string,",
				"          category_id as integer,",
				"          launch_date as date,",
				"          price as decimal(18,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source4",
				"source1 derive(sale_date = trim(sale_date),",
				"          store_id = substring(trim(store_id), 4),",
				"          product_id = substring(trim(product_id), 3),",
				"          quantity = trim(quantity)) ~> derivedColumn1",
				"derivedColumn1 cast(output(",
				"          sale_date as date 'dd-MM-yyyy',",
				"          store_id as integer,",
				"          product_id as integer,",
				"          quantity as integer",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1, source4 lookup(cast1@product_id == source4@product_id,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, source3 lookup(source1@sale_id == source3@sale_id,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          sale_id as string,",
				"          sale_date as date,",
				"          store_id as integer,",
				"          product_id as integer,",
				"          category_id as integer,",
				"          warranty_id as integer,",
				"          quantity as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          sale_date,",
				"          store_id,",
				"          product_id = cast1@product_id,",
				"          category_id,",
				"          warranty_id = claim_id,",
				"          quantity",
				"     )) ~> sink1"
			]
		}
	}
}